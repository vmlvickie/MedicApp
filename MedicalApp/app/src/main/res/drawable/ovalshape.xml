<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <item
        android:state_pressed="true">
        <shape
            android:shape="rectangle" >
            <corners
                android:radius="3dip" />
            <stroke
                android:width="1dip"
                android:color="#9ea3ff" />
            <gradient
                android:angle="-90"
                android:startColor="#9ea3ff"
                android:endColor="#689a92" />
        </shape>
    </item>
    <!--kkjkj-->
    <item
        android:state_focused="true">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="3dp"/>
            <stroke
                android:width="1dp"
                android:color="#9ea3ff"/>
            <solid
                android:color="#9ea3ff"/>
        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="3dip"/>
            <stroke
                android:width="1dp"
                android:color="#adc6ff" />
            <gradient
                android:angle="-90"
                android:startColor="#e5a5ff"
                android:endColor="#a4f3ff" />
        </shape>
    </item>


</selector>

