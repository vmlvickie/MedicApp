<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape
            android:shape="rectangle" >
            <corners
                android:radius="3dip" />
                <stroke
                   android:width="1dip"
                    android:color="#adc6ff"/>/>
                    <gradient
                        android:angle="-90"

                        android:startColor="#adc6ff"
                        android:endColor="#adc6ff" />
        </shape>
    </item>
<!--kkjkj-->
    <item
        android:state_focused="true">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="3dp"/>
            <stroke
                android:width="1dp"
                android:color="#adc6ff"/>
            <solid
                android:color="#adc6ff"/>
        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="3dip"/>
            <stroke
                android:width="1dp"
                android:color="#bbc3ff" />
            <gradient
                android:angle="-90"
                android:startColor="#10fffa"
                android:endColor="#adc6ff"
                 />
        </shape>
    </item>


</selector>
